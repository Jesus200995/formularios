# FormBuilder API Configuration

# App settings
APP_NAME="FormBuilder API"
APP_VERSION="1.0.0"
DEBUG=false

# Database (PostgreSQL)
DATABASE_URL="postgresql+asyncpg://user:password@localhost:5432/formularios"

# JWT Settings
SECRET_KEY="your-super-secret-key-change-this-in-production"
ALGORITHM="HS256"
ACCESS_TOKEN_EXPIRE_MINUTES=10080

# Upload settings
UPLOAD_DIR="uploads"
MAX_UPLOAD_SIZE=10485760

# CORS (comma separated origins)
CORS_ORIGINS="https://admindata.geodatos.com.mx,https://data.geodatos.com.mx"
